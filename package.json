{
  "name": "data-structures-and-algorithms",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -c .storybook -o storybook-dist",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"**/*.{js,ts,tsx}\"",
    "prettier-watch": "onchange \"**/*.{js,ts,tsx}\" -- prettier --write {{changed}}",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --collectCoverageFrom=\"src/**/src/*.{js,ts,tsx}\" --watchAll=false",
    "test:logs": "SHOW_LOGS=1 react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
